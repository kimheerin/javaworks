package classes.methods;

//계산기 자료형 정의
public class Calculator {
	
	//public Calculator() {}
	
	//메소드 정의 - 덧셈, 뺄셈, 곱셈, 나눗셈
	public int add(int n1, int n2) {	//void 아닌 int
			return n1 + n2;
	}

	public int sub(int n1, int n2) {
			return n1 - n2;
	}

	public int mul(int n1, int n2) {
		return n1 * n2;
	}
	
	public int div(int n1, int n2) {
		return n1 / n2;
	}
	
	
	
	
	
	
}
