package swtichexample;

public class SwitchCaseTest3 {

	public static void main(String[] args) {
		// 사칙연산 - 산술 기호
		// 변수 - num1, num2, operator
		// 2 * 3 = 6
		
		int num1 = 10;
		int num2 = 2;
		char operator = ''; //값을 초기화
		
		switch(operator) {
		case "+":
			result = num1 + num2
		}

	}

}
