package kr.or.iei.model;

//회원 class (자료형)
public class User {
	//void는 뭔가 받고 하고 사라져 | static 안 사라져 그래서 return
	//필드
	private String useid;
	private String pwd;
	private String name;
	
	//매개변수가 있는 생성자
	public User(String useid, String pwd, String name) {
		this.useid = useid;
		this.pwd = pwd;
		this.name = name;		
	}
	//get, set
	public void setUseid(String useid) {
		this.useid = useid;	
	}
	public String getUseid() {
		return useid;
	}
	public void setPwd(String pwd) {
		this.pwd = pwd;	
	}
	public String getPwd() {
		return pwd;
	}
	public void setName(String name) {
		this.name = name;	
	}
	public String getName() {
		return name;
	}
}