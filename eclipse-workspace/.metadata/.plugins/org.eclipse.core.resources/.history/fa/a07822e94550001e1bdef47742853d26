package classes;

//참고(래퍼런스) 자료형
public class Student {
	//클래스 내부에서 변수 = 필드(field)
	String name;	//학생 이름
	int studentId; 	//학번(101)
	int grade;		//학년
	
	//학생의 정보를 출력하는 메소드(함수)
	public void showStudentInfo() {
		System.out.println("\n이름 : " + name + 
				"\n학번 : " + studentId + "\n학년 : " + grade);
	}
	
	
	
	
	}

