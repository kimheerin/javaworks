package kr.or.iei.model;

public class User {
	private String name;
	private String id;
	private int pwd;
	//매개변수가 있는 생성자 : set 필요 X --> 이미 입력되어서
	//기본생성자 : set 필요 O
	public User(String name, String id, int pwd) {
		this.name = name;
		this.id = id;
		this.pwd = pwd;
	}
	//public void get~ : void 사용 시 끌고 가는 게 불가함(바로 출력)
	public String getName() {		
		return name;
	}
	public String getid() {		
		return id;
	}
	public int getpwd() {		
		return pwd;
	}

}
