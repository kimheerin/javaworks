package objectarray.dogs;

public class DogArray {

	public static void main(String[] args) {
		// Dog 인스탠스 4개를 생성해서 배열에 저장하고
		// 정보를 출력하세요(get()함수 사용
		Dog[] dogs = new Dog[4];

		dogs[0] = new Dog("진돗개");
		dogs[1] = new Dog("말티즈");
		dogs[2] = new Dog("웰시코기");
		dogs[3] = new Dog("닥스훈트");
		
		
		
	}

}
