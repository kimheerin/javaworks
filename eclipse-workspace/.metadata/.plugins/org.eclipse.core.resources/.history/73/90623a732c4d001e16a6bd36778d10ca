package constant;

public class PrintFormat {

	public static void main(String[] args) {
		//printf("문자열 포맷", 객체(변수))
		//정수 - %d, 실수 %f, 문자 %s
		//decimal(10진)
		
		int year = 2023;
		//System.out.println("올해는" + year + "년 입니다."); //아래와 같음
		System.out.printf("올해는 %d년입니다.", year);

		float weight = 63.7F;
		
		
		
	}

}
