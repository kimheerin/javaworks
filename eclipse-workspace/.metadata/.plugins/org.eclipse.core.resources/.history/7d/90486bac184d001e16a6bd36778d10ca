package ifexample;

public class ScorePointTest {

	public static void main(String[] args) {
		// 점수에 따른 학점 계산 프로그램
		// 변수 = score(정수형), grade(문자형)
		// A학점 - 90 ~ 100점
		// B학점 - 80 ~ 90점
		// C학점 - 70 ~ 80점
		// D학점 - 60 ~ 70점
		// F학점 - 60점 이하
		
		//입력
		int score = 85;
		char grade = 'A';	//char - 한 문자(홑따옴표)
		
		//연산
		if(score >= 90 && score <= 100) {
			grade = 'A';
			System.out.println(grade + "학점 입니다.");
		}else if(score > 80 && score < 80) 	
			grade = 'B';
		
			
		
		//출력
		
		
		
		
		
		
		
	}

}
