package transport;

public class BusPersonTest {

	public static void main(String[] args) {
		//사람 객체
		Person p1 = new Person("홍길동", 10000);
		Person p2 = new Person("김길동", 20000);
		//버스 객체
		Bus bus100 = new Bus(100);
		p1.takeBus(bus100);
		p2.takeBus(bus100);
		//출력
		p1.showPersonInfo();
		p2.showPersonInfo();
		bus100.showBusInfo();

	}

}
