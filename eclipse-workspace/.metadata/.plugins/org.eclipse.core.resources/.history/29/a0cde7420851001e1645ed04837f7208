package kr.or.iei.model;

//회원 class (자료형)
public class User {

		//필드(속성)
		private String id; 				//private = 접근 못하도록 하는 것(보안 문제)
		private String pwd; 
		private String name; 
		
		//set필드명(), get필드명()			//private 접근 불가이므로 set get 사용
		//접근하려면 public 써야 함
		public void setId(String id) {	//set = 입력	| id 입력
			this.id = id;
		}
		public String getId() {			//get = 출력 | id 출력
			return id;
		}
		
		
		
}
