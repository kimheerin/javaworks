package forexample;

public class ForEx1 {

	public static void main(String[] args) {
		/*
		 for(초기값; 조료값; 증감값){
		 	실행문
		 }
		 */
		
		// 1 ~ 10 출력
		for(int i=1; i<=10; i++) {
			System.out.print(i + " ");
		}
		System.out.println("");
		
		//아스키 코드값 알파벳 출력
		//'A' - 65, 'a' - 97, 0 - 48
		char alpha = 'A';
		System.out.println(alpha); // 기본 문자
		
		int val = 66;
		System.out.println((char)val); // 아스키 코드값 - (char)
		
		/*for(int num=65; num<=91; num++) {	//A - 65 ~ .... ?? - 91
			System.out.print((char)num + " ");
		}*/
		System.out.println();
		
		for(char ch= 65; ch<123; ch++) {			// 위보다 더 쉬운 식
			System.out.print(ch + " ");
		}
		
		//for(int ch=65; ch<=96; ch++) {

		//}
		
	}

}
