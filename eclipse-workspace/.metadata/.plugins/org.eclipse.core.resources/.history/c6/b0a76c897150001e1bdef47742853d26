package objectarray.books;

public class BookTest {

	public static void main(String[] args) {
		Book[] lib = new Book[3];
		
		for(int i=0; i<lib.length; i++) {
			System.out.println(lib[1]);
		}
		
		//객체 생성(배열에 저장)
		lib[0] = new Book("자바 프로그래밍 입문", "박은총");
		
		//출력
		lib[0].showBookInfo();
		
		
	}

}
