package forexample;

public class ForEx1 {

	public static void main(String[] args) {
		/*
		 for(초기값; 조료값; 증감값){
		 	실행문
		 }
		 */
		
		// 1 ~ 10 출력
		for(int i=1; i<=10; i++) {
			System.out.print(i + " ");
		}
		
		//아스키 코드값 알파벳 출력
		//'A' - 65, 'a' - 97, 0 - 48
		char alpha = 'A';
		System.out.println(alpha);
		
		
		
		
		//for(int ch=65; ch<=96; ch++) {

		//}
		
	}

}
