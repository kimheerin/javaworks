package arrays;

public class ArrayGugudan {

	public static void main(String[] args) {
		// 구구단 3단 출력
		// 1. 기본
		int dan = 3;
		System.out.println("3단 시작");
		for(int i=1; i<10; i++) {
			System.out.println(dan + "x" + i + "=" + dan * i);
		}
		System.out.println("==================================");
		
	}

}
