package arrays;

public class ArrayTest1 {

	public static void main(String[] args) {
		// 변수
		String car1 = "Sonata";
		String car2 = "Morning";
		String car3 = "K8";
		
		System.out.println(car1);
		System.out.println(car2);
		System.out.println(car3);
		
		System.out.println("======================");
		
		//배열
		//직접 저장 - 중괄호{} 사용
		String[] cars = {"Sonata", "Moring", "K8"};
		System.out.println(cars[0]);
		
		
		
		

	}//class

}//main
